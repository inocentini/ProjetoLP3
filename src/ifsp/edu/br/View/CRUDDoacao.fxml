<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="499.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ifsp.edu.br.Control.CRUDDoacaoController">
   <children>
      <Label fx:id="labGerenDoa" layoutX="118.0" layoutY="30.0" text="Cadastramento de Doação">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="tableAnimal" layoutX="52.0" layoutY="176.0" prefHeight="135.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="tableAnimalId" prefWidth="36.0" text="id" />
          <TableColumn fx:id="tableAnimalApelido" prefWidth="163.0" text="Apelido" />
        </columns>
      </TableView>
      <TableView fx:id="tableProduto" layoutX="257.0" layoutY="176.0" prefHeight="135.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="tableProdutoId" prefWidth="39.0" text="id" />
          <TableColumn fx:id="tableProdutoDesc" prefWidth="160.0" text="Descrição" />
        </columns>
      </TableView>
      <Label fx:id="labProdutos" layoutX="258.0" layoutY="150.0" text="Produtos:" />
      <Button fx:id="btnInserirProd" layoutX="326.0" layoutY="145.0" mnemonicParsing="false" onAction="#handleBtnInserirProduto" text="Inserir" />
      <Label fx:id="labUsers" layoutX="52.0" layoutY="72.0" text="Usuários:" />
      <ComboBox fx:id="cbUser" layoutX="118.0" layoutY="67.0" prefHeight="26.0" prefWidth="207.0" />
      <Label fx:id="labDate" layoutX="52.0" layoutY="113.0" text="Data:" />
      <Label fx:id="labDataDoa" layoutX="120.0" layoutY="113.0" text="Label" />
      <Label fx:id="labAnimais" layoutX="52.0" layoutY="150.0" text="Animais:" />
      <Button fx:id="btnInserirAnimal" layoutX="115.0" layoutY="145.0" mnemonicParsing="false" onAction="#handleBtnInserirAnimal" text="Inserir" />
      <Button fx:id="btnAdicionar" layoutX="284.0" layoutY="345.0" mnemonicParsing="false" onAction="#handleAdicionarDoa" text="Cadastrar" />
      <Button fx:id="btnVoltar" layoutX="378.0" layoutY="345.0" mnemonicParsing="false" onAction="#handleBtnVoltar" prefHeight="26.0" prefWidth="82.0" text="Cancelar" />
   </children>
</AnchorPane>
